{
    "name": "discord-reminder",
    "version": "0.0.1",
    "description": "A discord bot to remind things at certain point in time in the future.",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf dist/",
        "copy-files": "copyfiles src/assets/* dist",
        "lint": "tslint --project . --fix",
        "start": "npm run clean && tsc && npm run copy-files && node dist/index.js",
        "build": "npm run lint && npm run start",
        "git-push": "npm run clean && npm run lint && tsc && git add . && git commit -m 'autocommit' && git push"
    },
    "devDependencies": {
        "@types/debug": "0.0.30",
        "@types/node": "^9.6.5",
        "@types/node-schedule": "^1.3.1",
        "@types/yamljs": "^0.2.30",
        "tslint-eslint-rules": "^5.1.0"
    },
    "dependencies": {
        "discord.js": "^11.3.2",
        "dotenv": "^8.2.0",
        "moment": "^2.29.1",
        "node-schedule": "^2.0.0",
        "tslint": "^5.9.1",
        "rimraf": "^3.0.2",
        "copyfiles": "^2.4.1",
        "typescript": "^4.1.5"
    },
    "keywords": [
        "discord bot",
        "discord"
    ],
    "author": "likefurnis",
    "license": "GPL-3.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/likefurnis/discord-reminder.git"
    },
    "homepage": "https://github.com/likefurnis/discord-reminder#readme"
}
